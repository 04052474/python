    "def multiply(m,n):\n",
    "    if n==1:\n",
    "        return m\n",
    "    return m+multiply(m,n-1)\n",
    "    \n",
    "m=int(input('輸入m:'))\n",
    "n=int(input('輸入n:'))\n",
    "print(\"輸入 multiply(\",m,\",\",n,\")\")\n",
    "print(multiply(m,n))"
