    "import numpy as np\n",
    "def mindivmax(arr):\n",
    "    A=np.max(arr,axis=1)\n",
    "    B=np.min(arr,axis=1)#each row\n",
    "    return B/A\n",
    "mindivmax(np.array([[9, 9, 4],[8, 8, 1],[5, 3, 6],[3, 3, 3],[2, 1, 9]]))\n"
   ]
