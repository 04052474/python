    "def uniq_items(lst):\n",
    "    uniqItems=[]\n",
    "    for i in lst:\n",
    "        if i not in uniqItems:\n",
    "            uniqItems.append(i)\n",
    "    return uniqItems\n",
    "print(uniq_items([12, 16, 2, 16]))\n",
    "print(uniq_items([13, 13, 4, 23]))\n",
    "print(uniq_items([12, 3, 12, 3, 24]))\n",
    "print(uniq_items([26, 31, 132]))\n",
    "\n"
   ]
